{
    "root": true,
    "env": {
        "node": true,
        "es6": true
    },
    "extends": ["eslint:recommended", "plugin:@typescript-eslint/recommended", "plugin:@next/next/recommended"],
    "parser": "@typescript-eslint/parser",
    "plugins": ["@typescript-eslint", "prettier"],
    "rules": {
        "@typescript-eslint/ban-ts-comment": "off"
    }
}
